<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
  <hibernate-mapping>
    
    <class name="com.jidian.ssh.entity.Sort" table="t_sort"> 
    	<id name="s_id" column="s_id">
    		<generator class="identity"></generator>
    	</id>
    	
    	<property name="s_name" length="200" type="java.lang.String" ></property>
    	<property name="s_remark" length="200" type="java.lang.String" ></property>
    	<property name="s_is"  type="java.lang.Integer" ></property>
    	
    </class>
    
  </hibernate-mapping>
